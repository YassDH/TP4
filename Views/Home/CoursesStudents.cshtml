@model IEnumerable<Student>

<div class="text-center">
    <h1 class="display-4">Students of the @ViewBag.CourseName Course </h1>
</div>

<div class="container">
    @if(Model.Count() == 0){
        <div class="alert alert-danger" role="alert">
            This Course has no Students
        </div>
    }else{
        <div class="row">   
            @foreach( Student item in Model) {
                <div class="col mb-3">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">@item.first_name @item.last_name</h5>
                            <p>Phone : @item.phone_number</p>
                            <p>University : @item.university</p>
                            <p>TimeStamp : @item.timestamp</p>
                        </div>
                    </div>
                </div>
            }
        </div>
    }
</div>